@media not screen and (min-width: 768px)
  .navbar-box
    position fixed
    width 100%
    height 45px
    top 0
    left 0
    background-color white
    .navbar-logo
      width 45px
      position absolute
      left 0
      top 0
      z-index 102
    .navbar-expanded
      height 45px
      z-index 101
      .iconfont
        width 45px
        right 0
        top 0
        padding 0
        line-height 45px
        color #ea6f5a
  #main-context
    margin-top 5px
    margin-left 0
    height none
    #sidelogo
      display none
  .posts-all, #backend-container
    padding-left 0
    margin-top 45px
    margin-left 0
      